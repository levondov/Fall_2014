\subsection{Field Oxide Capacitor, 3} %%%%%%% Field Oxide Capacitors 3

\subsubsection{Measurement Setup}
\begin{figure}[H]
\centering
\begin{tikzpicture}
\node at (5,5) {\includegraphics[width=250pt]{Device_setup/device3a2.pdf}};
